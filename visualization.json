{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "A scatter plot showing the relationship between year acquired and year constructed.",
  "data": {
    "url": "building_inventory.csv"
  },
  "mark": "circle",
  "encoding": {
    "x": {
      "field": "Year Acquired",
      "type": "quantitative",
      "title": "Year Acquired"
    },
    "y": {
      "field": "Year Constructed",
      "type": "quantitative",
      "title": "Year Constructed"
    },
    "color": {
      "field": "Year Acquired",
      "type": "quantitative",
      "legend": {
        "title": "Year Acquired"
      }
    },
    "size": {
      "value": 60
    }
  }
}
